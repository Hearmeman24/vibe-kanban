  "query": "SELECT id as \"id!: Uuid\", project_id as \"project_id!: Uuid\", title, description, status as \"status!: TaskStatus\", parent_workspace_id as \"parent_workspace_id: Uuid\", shared_task_id as \"shared_task_id: Uuid\", assignee, agent_metadata, created_at as \"created_at!: DateTime<Utc>\", updated_at as \"updated_at!: DateTime<Utc>\"\n               FROM tasks\n               WHERE project_id = $1\n                 AND (title LIKE $2 OR description LIKE $2)\n               ORDER BY\n                 CASE WHEN title LIKE $2 THEN 0 ELSE 1 END,\n                 updated_at DESC\n               LIMIT $3 OFFSET $4",
