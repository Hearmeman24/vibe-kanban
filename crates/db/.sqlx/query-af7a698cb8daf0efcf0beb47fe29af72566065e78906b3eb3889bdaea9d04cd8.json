  "query": "SELECT id as \"id!: Uuid\", webhook_id as \"webhook_id!: Uuid\", event_type, payload, status as \"status!: DeliveryStatus\", attempts as \"attempts!: i64\", last_error, next_retry_at as \"next_retry_at: DateTime<Utc>\", created_at as \"created_at!: DateTime<Utc>\", delivered_at as \"delivered_at: DateTime<Utc>\"\n               FROM webhook_deliveries\n               WHERE status = 'pending'\n                  OR (status = 'retrying' AND (next_retry_at IS NULL OR next_retry_at <= datetime('now', 'subsec')))\n               ORDER BY created_at ASC",
